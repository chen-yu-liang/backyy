
<div class="pageContent" style="overflow:hidden;">
	<form method="post" action="${(projectName)!}/?spm=${(urlEncrption('/${(action)!}/save'))!}" class="pageForm required-validate" onsubmit="return validateCallback(this, dialogAjaxDone);">
		<div class="pageFormContent" layoutH="50" style="background-color:#ecf2f4;overflow:hidden;padding-bottom:0">
			<fieldset>
				<p>
					<label style="width:60px;">交易金额</label>
					<input type="text" name="amount" value="${(clientResponsedata.amount)!}"/>
				</p>
				<p>
					<label style="width:60px;">支付结果通知时间</label>
					<input type="text" name="time" value="${(clientResponsedata.time)!}"/>
				</p>
				<p>
					<label style="width:60px;">商户订单号</label>
					<input type="text" name="reqdid" value="${(clientResponsedata.reqdid)!}"/>
				</p>
				<p>
					<label style="width:60px;">支付状态</label>
					<input type="text" name="trade_state" value="${(clientResponsedata.trade_state)!}"/>
				</p>
				<p>
					<label style="width:60px;">支付结果通知状态 0成功 1失败</label>
					<input type="text" name="status" value="${(clientResponsedata.status)!}"/>
				</p>
				<p>
					<label style="width:60px;">交易类型</label>
					<input type="text" name="service" value="${(clientResponsedata.service)!}"/>
				</p>
				<p>
					<label style="width:60px;">交易时间</label>
					<input type="text" name="localdate" value="${(clientResponsedata.localdate)!}"/>
				</p>
				<p>
					<label style="width:60px;">返回码</label>
					<input type="text" name="res_code" value="${(clientResponsedata.res_code)!}"/>
				</p>
				<p>
					<label style="width:60px;">支付结果通知次数</label>
					<input type="text" name="number" value="${(clientResponsedata.number)!}"/>
				</p>
				<p>
					<label style="width:60px;">商户号</label>
					<input type="text" name="client_no" value="${(clientResponsedata.client_no)!}"/>
				</p>
				<p>
					<label style="width:60px;">支付时间</label>
					<input type="text" name="pay_time" value="${(clientResponsedata.pay_time)!}"/>
				</p>
				<p>
					<label style="width:60px;">唯一凭证号</label>
					<input type="text" name="serial" value="${(clientResponsedata.serial)!}"/>
				</p>
				<input type="hidden" name="id" value="${(baseinfoClass.id)!}">
			</fieldset>
		</div>
		<div class="formBar">
			<ul>
				<li><div class="buttonActive"><div class="buttonContent"><button type="submit">保存</button></div></div></li>
				<li><div class="button"><div class="buttonContent"><button type="button" class="close">取消</button></div></div></li>
			</ul>
		</div>
	</form>
</div>

