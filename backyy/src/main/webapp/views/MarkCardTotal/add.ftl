
<div class="pageContent" style="overflow:hidden;">
	<form method="post" action="${(projectName)!}/?spm=${(urlEncrption('/${(action)!}/save'))!}" class="pageForm required-validate" onsubmit="return validateCallback(this, dialogAjaxDone);">
		<div class="pageFormContent" layoutH="50" style="background-color:#ecf2f4;overflow:hidden;padding-bottom:0">
			<fieldset>
				<p>
					<label style="width:60px;">分卡起始号段</label>
					<input type="text" name="mark_client_no_start_num" value="${(markCardTotal.mark_client_no_start_num)!}"/>
				</p>
				<p>
					<label style="width:60px;">批次号</label>
					<input type="text" name="mark_batch_no" value="${(markCardTotal.mark_batch_no)!}"/>
				</p>
				<p>
					<label style="width:60px;">分卡数量</label>
					<input type="text" name="mark_card_num" value="${(markCardTotal.mark_card_num)!}"/>
				</p>
				<p>
					<label style="width:60px;">分卡结束号段</label>
					<input type="text" name="mark_client_no_end_num" value="${(markCardTotal.mark_client_no_end_num)!}"/>
				</p>
				<p>
					<label style="width:60px;">分卡时间</label>
					<input type="text" name="mark_add_time" value="${(markCardTotal.mark_add_time)!}"/>
				</p>
				<p>
					<label style="width:60px;">代理商</label>
					<input type="text" name="agent_no" value="${(markCardTotal.agent_no)!}"/>
				</p>
				<input type="hidden" name="id" value="${(baseinfoClass.id)!}">
			</fieldset>
		</div>
		<div class="formBar">
			<ul>
				<li><div class="buttonActive"><div class="buttonContent"><button type="submit">保存</button></div></div></li>
				<li><div class="button"><div class="buttonContent"><button type="button" class="close">取消</button></div></div></li>
			</ul>
		</div>
	</form>
</div>

