
<div class="pageContent" style="overflow:hidden;">
	<form method="post" action="${(projectName)!}/?spm=${(urlEncrption('/${(action)!}/save'))!}" class="pageForm required-validate" onsubmit="return validateCallback(this, dialogAjaxDone);">
		<div class="pageFormContent" layoutH="50" style="background-color:#ecf2f4;overflow:hidden;padding-bottom:0">
			<fieldset>
				<p>
					<label style="width:60px;">总交易金额</label>
					<input type="text" name="amount" value="${(agentProfit.amount)!}"/>
				</p>
				<p>
					<label style="width:60px;"></label>
					<input type="text" name="id" value="${(agentProfit.id)!}"/>
				</p>
				<p>
					<label style="width:60px;">总结算金额</label>
					<input type="text" name="clear_amount" value="${(agentProfit.clear_amount)!}"/>
				</p>
				<p>
					<label style="width:60px;">代理商成本</label>
					<input type="text" name="agent_cost" value="${(agentProfit.agent_cost)!}"/>
				</p>
				<p>
					<label style="width:60px;">清算日期</label>
					<input type="text" name="qilui_date" value="${(agentProfit.qilui_date)!}"/>
				</p>
				<p>
					<label style="width:60px;">代理商编号</label>
					<input type="text" name="agent_no" value="${(agentProfit.agent_no)!}"/>
				</p>
				<p>
					<label style="width:60px;">代理分润</label>
					<input type="text" name="agent_profit" value="${(agentProfit.agent_profit)!}"/>
				</p>
				<p>
					<label style="width:60px;">交易笔数</label>
					<input type="text" name="count_num" value="${(agentProfit.count_num)!}"/>
				</p>
				<p>
					<label style="width:60px;">交易日期</label>
					<input type="text" name="deal_date" value="${(agentProfit.deal_date)!}"/>
				</p>
				<input type="hidden" name="id" value="${(baseinfoClass.id)!}">
			</fieldset>
		</div>
		<div class="formBar">
			<ul>
				<li><div class="buttonActive"><div class="buttonContent"><button type="submit">保存</button></div></div></li>
				<li><div class="button"><div class="buttonContent"><button type="button" class="close">取消</button></div></div></li>
			</ul>
		</div>
	</form>
</div>

