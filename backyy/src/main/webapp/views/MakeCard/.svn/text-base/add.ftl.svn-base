
<div class="pageContent" style="overflow:hidden;">
	<form method="post" action="${(projectName)!}/?spm=${(urlEncrption('/${(action)!}/save'))!}" class="pageForm required-validate" onsubmit="return validateCallback(this, dialogAjaxDone);">
		<div class="pageFormContent" layoutH="50" style="background-color:#ecf2f4;overflow:hidden;padding-bottom:0">
			<fieldset>
				<p>
					<label style="width:60px;">主键</label>
					<input type="text" name="id" value="${(makeCard.id)!}"/>
				</p>
				<p>
					<label style="width:60px;">分卡时间</label>
					<input type="text" name="mark_card_time" value="${(makeCard.mark_card_time)!}"/>
				</p>
				<p>
					<label style="width:60px;">商户号</label>
					<input type="text" name="client_no" value="${(makeCard.client_no)!}"/>
				</p>
				<p>
					<label style="width:60px;">制卡时间</label>
					<input type="text" name="add_time" value="${(makeCard.add_time)!}"/>
				</p>
				<p>
					<label style="width:60px;">代理商编号</label>
					<input type="text" name="agent_no" value="${(makeCard.agent_no)!}"/>
				</p>
				<input type="hidden" name="id" value="${(baseinfoClass.id)!}">
			</fieldset>
		</div>
		<div class="formBar">
			<ul>
				<li><div class="buttonActive"><div class="buttonContent"><button type="submit">保存</button></div></div></li>
				<li><div class="button"><div class="buttonContent"><button type="button" class="close">取消</button></div></div></li>
			</ul>
		</div>
	</form>
</div>

