
<div class="pageContent" style="overflow:hidden;">
	<form method="post" action="${(projectName)!}/?spm=${(urlEncrption('/${(action)!}/save'))!}" class="pageForm required-validate" onsubmit="return validateCallback(this, dialogAjaxDone);">
		<div class="pageFormContent" layoutH="50" style="background-color:#ecf2f4;overflow:hidden;padding-bottom:0">
			<fieldset>
				<p>
					<label style="width:60px;">备注</label>
					<input type="text" name="operation_remark" value="${(balancePayAll.operation_remark)!}"/>
				</p>
				<p>
					<label style="width:60px;">操作序号</label>
					<input type="text" name="ordinal" value="${(balancePayAll.ordinal)!}"/>
				</p>
				<p>
					<label style="width:60px;">操作日期</label>
					<input type="text" name="add_date" value="${(balancePayAll.add_date)!}"/>
				</p>
				<p>
					<label style="width:60px;">商户编号</label>
					<input type="text" name="client_no" value="${(balancePayAll.client_no)!}"/>
				</p>
				<p>
					<label style="width:60px;">操作人</label>
					<input type="text" name="userinfo" value="${(balancePayAll.userinfo)!}"/>
				</p>
				<p>
					<label style="width:60px;">变更后金额</label>
					<input type="text" name="amount_after" value="${(balancePayAll.amount_after)!}"/>
				</p>
				<p>
					<label style="width:60px;">商户账户</label>
					<input type="text" name="account_no" value="${(balancePayAll.account_no)!}"/>
				</p>
				<p>
					<label style="width:60px;">变更流水号</label>
					<input type="text" name="serial_id" value="${(balancePayAll.serial_id)!}"/>
				</p>
				<p>
					<label style="width:60px;"></label>
					<input type="text" name="id" value="${(balancePayAll.id)!}"/>
				</p>
				<p>
					<label style="width:60px;">变动金额</label>
					<input type="text" name="amount" value="${(balancePayAll.amount)!}"/>
				</p>
				<p>
					<label style="width:60px;">变更前金额</label>
					<input type="text" name="amount_before" value="${(balancePayAll.amount_before)!}"/>
				</p>
				<p>
					<label style="width:60px;">操作类型 D001代理充值 ，D002代理提现</label>
					<input type="text" name="operation" value="${(balancePayAll.operation)!}"/>
				</p>
				<p>
					<label style="width:60px;">变更前_验证值</label>
					<input type="text" name="amount_before_key" value="${(balancePayAll.amount_before_key)!}"/>
				</p>
				<p>
					<label style="width:60px;">变更后_验证值</label>
					<input type="text" name="amount_after_key" value="${(balancePayAll.amount_after_key)!}"/>
				</p>
				<input type="hidden" name="id" value="${(baseinfoClass.id)!}">
			</fieldset>
		</div>
		<div class="formBar">
			<ul>
				<li><div class="buttonActive"><div class="buttonContent"><button type="submit">保存</button></div></div></li>
				<li><div class="button"><div class="buttonContent"><button type="button" class="close">取消</button></div></div></li>
			</ul>
		</div>
	</form>
</div>

