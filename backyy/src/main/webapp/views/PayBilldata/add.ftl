
<div class="pageContent" style="overflow:hidden;">
	<form method="post" action="${(projectName)!}/?spm=${(urlEncrption('/${(action)!}/save'))!}" class="pageForm required-validate" onsubmit="return validateCallback(this, dialogAjaxDone);">
		<div class="pageFormContent" layoutH="50" style="background-color:#ecf2f4;overflow:hidden;padding-bottom:0">
			<fieldset>
				<p>
					<label style="width:60px;"></label>
					<input type="text" name="id" value="${(payBilldata.id)!}"/>
				</p>
				<p>
					<label style="width:60px;">代付金额</label>
					<input type="text" name="amount" value="${(payBilldata.amount)!}"/>
				</p>
				<p>
					<label style="width:60px;">流水号</label>
					<input type="text" name="rrno" value="${(payBilldata.rrno)!}"/>
				</p>
				<p>
					<label style="width:60px;">代付时间</label>
					<input type="text" name="paid_date" value="${(payBilldata.paid_date)!}"/>
				</p>
				<p>
					<label style="width:60px;"></label>
					<input type="text" name="number_no" value="${(payBilldata.number_no)!}"/>
				</p>
				<p>
					<label style="width:60px;">实际代付金额</label>
					<input type="text" name="money" value="${(payBilldata.money)!}"/>
				</p>
				<p>
					<label style="width:60px;">对账文件编号</label>
					<input type="text" name="bill_no" value="${(payBilldata.bill_no)!}"/>
				</p>
				<p>
					<label style="width:60px;"></label>
					<input type="text" name="batch_no" value="${(payBilldata.batch_no)!}"/>
				</p>
				<input type="hidden" name="id" value="${(baseinfoClass.id)!}">
			</fieldset>
		</div>
		<div class="formBar">
			<ul>
				<li><div class="buttonActive"><div class="buttonContent"><button type="submit">保存</button></div></div></li>
				<li><div class="button"><div class="buttonContent"><button type="button" class="close">取消</button></div></div></li>
			</ul>
		</div>
	</form>
</div>

