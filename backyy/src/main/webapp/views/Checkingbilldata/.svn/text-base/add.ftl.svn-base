
<div class="pageContent" style="overflow:hidden;">
	<form method="post" action="${(projectName)!}/?spm=${(urlEncrption('/${(action)!}/save'))!}" class="pageForm required-validate" onsubmit="return validateCallback(this, dialogAjaxDone);">
		<div class="pageFormContent" layoutH="50" style="background-color:#ecf2f4;overflow:hidden;padding-bottom:0">
			<fieldset>
				<p>
					<label style="width:60px;">交易金额</label>
					<input type="text" name="trade_money" value="${(checkingbilldata.trade_money)!}"/>
				</p>
				<p>
					<label style="width:60px;">商户号</label>
					<input type="text" name="client_no" value="${(checkingbilldata.client_no)!}"/>
				</p>
				<p>
					<label style="width:60px;">交易状态 （0成功1失败）</label>
					<input type="text" name="trade_status" value="${(checkingbilldata.trade_status)!}"/>
				</p>
				<p>
					<label style="width:60px;">交易类型</label>
					<input type="text" name="trade_type" value="${(checkingbilldata.trade_type)!}"/>
				</p>
				<p>
					<label style="width:60px;">交易卡号</label>
					<input type="text" name="pan" value="${(checkingbilldata.pan)!}"/>
				</p>
				<p>
					<label style="width:60px;">结算名</label>
					<input type="text" name="account_name" value="${(checkingbilldata.account_name)!}"/>
				</p>
				<p>
					<label style="width:60px;"></label>
					<input type="text" name="id" value="${(checkingbilldata.id)!}"/>
				</p>
				<p>
					<label style="width:60px;">手续费</label>
					<input type="text" name="trade_charage" value="${(checkingbilldata.trade_charage)!}"/>
				</p>
				<p>
					<label style="width:60px;">撤销表示 0正常数据 1撤销 2冲正</label>
					<input type="text" name="revoke_num" value="${(checkingbilldata.revoke_num)!}"/>
				</p>
				<p>
					<label style="width:60px;">交易日期</label>
					<input type="text" name="trade_date" value="${(checkingbilldata.trade_date)!}"/>
				</p>
				<p>
					<label style="width:60px;">结算金额</label>
					<input type="text" name="clear_money" value="${(checkingbilldata.clear_money)!}"/>
				</p>
				<p>
					<label style="width:60px;">对账参考号</label>
					<input type="text" name="reference_no" value="${(checkingbilldata.reference_no)!}"/>
				</p>
				<p>
					<label style="width:60px;">交易时间</label>
					<input type="text" name="trade_time" value="${(checkingbilldata.trade_time)!}"/>
				</p>
				<p>
					<label style="width:60px;">文件编号</label>
					<input type="text" name="bill_no" value="${(checkingbilldata.bill_no)!}"/>
				</p>
				<p>
					<label style="width:60px;">通道编号</label>
					<input type="text" name="bid" value="${(checkingbilldata.bid)!}"/>
				</p>
				<p>
					<label style="width:60px;">交易流水</label>
					<input type="text" name="serial" value="${(checkingbilldata.serial)!}"/>
				</p>
				<input type="hidden" name="id" value="${(baseinfoClass.id)!}">
			</fieldset>
		</div>
		<div class="formBar">
			<ul>
				<li><div class="buttonActive"><div class="buttonContent"><button type="submit">保存</button></div></div></li>
				<li><div class="button"><div class="buttonContent"><button type="button" class="close">取消</button></div></div></li>
			</ul>
		</div>
	</form>
</div>

